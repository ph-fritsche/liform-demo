{
    "name": "pitch/liform-demo",
    "description": "Demo and tests for Liform",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Philipp Fritsche",
            "email": "ph.fritsche@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "test/"
        }
    },
    "require": {
        "symfony/flex": "^1.7",
        "symfony/webpack-encore-bundle": "^1.7",
        "pitch/liform": "dev-master",
        "symfony/console": "*",
        "symfony/dotenv": "*",
        "symfony/form": "*",
        "symfony/framework-bundle": "*",
        "symfony/twig-bundle": "*",
        "limenius/react-bundle": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "test": "phpunit"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.*"
        }
    }
}
